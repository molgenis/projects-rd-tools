{
  "name": "rdtools",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/molgenis/projects-rd-tools",
  "author": "davidruvolo51",
  "license": "MIT",
  "scripts": {
    "build": "python setup.py sdist bdist_wheel",
    "check": "twine check dist/*",
    "install": "pip install dist/rdtools-0.0.0.tar.gz",
    "deploy:prod": "twine upload dist/*",
    "deploy:test": "twine upload --repository testpypi dist/*",
    "bump:patch": "bump2version --current-version 0.0.0 patch package.json rdtools/__version__.py --allow-dirty",
    "bump:minor": "bump2version --current-version 0.0.0 minor package.json rdtools/__version__.py",
    "bump:major": "bump2version --current-version 0.0.0 major package.json rdtools/__version__.py"
  }
}